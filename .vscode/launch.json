{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Django",
      "type": "python",
      "request": "launch",
      "stopOnEntry": false,
      "program": "${workspaceRoot}/music_controller/manage.py",
      "args": ["runserver", "1818"],
      "debugOptions": [
        "WaitOnAbnormalExit",
        "WaitOnNormalExit",
        "RedirectOutput",
        "DjangoDebugging"
      ],
      "env": {
        "DJANGO_DEBUG": "True",
        "DJANGO_SETTINGS_MODULE": "settings"
      }
    }
  ]
}
